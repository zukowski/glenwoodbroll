<h2>Sign In</h2>
<%= form_for(User.new, :url => new_user_session_path) do |f| %>
  <%= f.label :email %><br />
  <%= f.email_field :email %><br />
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />
  <%= f.check_box :remember_me %> <%= f.label :remember_me %>
  <%= f.submit "Sign In" %>
  <% link_to "Forgot your password?", new_user_password_path %>
<% end %>
